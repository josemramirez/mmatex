Cell["\<\
Clear @@ DeleteElements[Names@\"`*\", {\"check1a\", \"mmtxClass\", \"nbmmatex\
\"}];
(*- DBv1 -*)
apiMMaTeX1 = \\
CloudObject[\"https://www.wolframcloud.com/obj/dbc639cc-93d6-4c26-876f-\
e5320654\\
c70a\"]; 

(*== Introduce tu Key de MMaTeX - AI aqui !!==*)
If [ValueQ[check1a], Print[\"La sesion continua...\"],
check1a = URLExecute[apiMMaTeX1, {\"myKey\" -> InputString[\"Por favor \
introduzca una llave (key) valida: \", \"UNITECH-XDE1038\"]}]];

(*== If Pass ==*)
If[StringContainsQ[check1a, \"_licencia\"], mmtxDB = \\
Association[ToExpression[check1a]];

(*== Funciones adicionales de AI ==*)
aiTeX = \"https://raw.githubusercontent.com/josemramirez/mmatex/main/\
AIPackage_v7.nb\"; \\
(*== Requiere conexion a Internet, si no pide el notebook offline ==*)
NotebookEvaluate[aiTeX];

(*- NoteBook -*) 
nbmmatex = EvaluationNotebook[];
(* -- Localizo donde esta update -- *)
LocateUdpdate = Position[(NotebookRead /@ #)[[;; , 2]], \"Title\", 1][[;; , \
1]] &;
RunCells = LocateUdpdate@Cells[CellStyle -> {\"Input\" | \"Title\"}];
Echo[\": Es el numero de inputs hacia abajo |\", RunCells[[1]] - 1];
RunCellsBellow[nbmmatex, RunCells[[1]] - 1];
Print[Corriendo...]; ,

(*== If Not Pass ==*)
Print[mmtxDB]]\
\>", "Input",
 Editable->False,
 Deletable->False,
 CellOpen->False,
 Background->RGBColor[1, 0.9, 0.8],
